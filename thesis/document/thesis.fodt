   <text:p text:style-name="P43"><text:tab/>\include<text:span text:style-name="T61">graphics</text:span>[width=<text:span text:style-name="T61">0.8</text:span>\textwidth]{../<text:span text:style-name="T61">images/</text:span>dcf<text:span text:style-name="T71">CarrierEnvelope</text:span>}</text:p>
   <text:p text:style-name="P43"><text:tab/>\caption<text:span text:style-name="T71">[Obwiednia sygnau DCF77]</text:span>{<text:span text:style-name="T71">Obwiednia sygnau DCF77\cite[s. 13]{bauch09dcf}</text:span>}</text:p>
